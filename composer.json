{
    "name": "stewie/user-bundle",
    "type": "symfony-bundle",
    "description": "A user bundle for Symfony5",
    "authors" : [{
        "name" : "Stefan Wiegmann",
        "email" : "mail@stefanwiegmann.de"
    }],
    "keywords" : [
        "user bundle symfony symfony5"
    ],
    "license" : [
        "MIT"
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "*",
        "symfony/yaml": "5.0.*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "knplabs/knp-paginator-bundle": "^5.1",
        "sensio/framework-extra-bundle": "^5.1",
        "liip/imagine-bundle": "^2.3",
        "vich/uploader-bundle": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Stewie\\UserBundle\\": ""
        }
    },
    "scripts": {
        "config-scripts": {
            "stewie:user:configure": "symfony-cmd"
        },
        "post-install-cmd": [
            "@config-scripts"
        ],
        "post-update-cmd": [
            "@config-scripts"
        ]
    },
    "extra" : {
    "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
